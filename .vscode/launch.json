{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"preLaunchTask": "build:vscode-extension",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/apps/vscode",
				"--disable-extensions"
			],
			"outFiles": ["${workspaceFolder}/apps/vscode/dist/**/*.cjs"]
		}
	]
}
