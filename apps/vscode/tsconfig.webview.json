{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"types": ["svelte", "vite/client"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"target": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	},
	"include": ["src/webview/**/*"],
	"exclude": ["node_modules"]
}
